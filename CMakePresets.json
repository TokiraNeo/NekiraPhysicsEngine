{
  "version": 6,
  "configurePresets": [
    {
      "name": "vcpkg",
      "hidden": true,
      "binaryDir": "${sourceDir}/build",
      "cacheVariables": {
        "CMAKE_EXPORT_COMPILE_COMMANDS": "YES",
        "CMAKE_CXX_STANDARD": "20",
        "CMAKE_CXX_STANDARD_REQUIRED": "YES",
        "CMAKE_TOOLCHAIN_FILE": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake",
        "QT_CMAKE_PATH": ""
      },
      "environment": {
        "VCPKG_ROOT": ""
      }
    }
  ]
}